{
  "$schema": "../app-info-schema.json",
  "name": "PostgreSQL",
  "id": "postgres",
  "available": true,
  "short_desc": "PostgreSQL is a powerful, open source object-relational database",
  "author": "PostgreSQL Global Development Group",
  "port": 5432,
  "categories": ["development", "data"],
  "description": "PostgreSQL is a powerful, open source object-relational database system with over 35 years of active development that has earned it a strong reputation for reliability, feature robustness, and performance.",
  "tipi_version": 7,
  "version": "latest",
  "source": "https://github.com/postgres/postgres",
  "website": "https://www.postgresql.org/",
  "exposable": true,
  "dynamic_config": true,
  "no_gui": true,
  "form_fields": [
    {
      "type": "text",
      "label": "Database Name",
      "max": 63,
      "min": 1,
      "required": true,
      "env_variable": "POSTGRES_DB"
    },
    {
      "type": "text",
      "label": "Admin Username",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "POSTGRES_USER"
    },
    {
      "type": "password",
      "label": "Admin Password",
      "max": 128,
      "min": 10,
      "required": true,
      "env_variable": "POSTGRES_PASSWORD"
    }
  ],
  "supported_architectures": ["arm64", "amd64"]
}
